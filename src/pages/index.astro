<script>
	import p5 from 'p5';
	const mySketch = (p) => {
		p.setup = () => {
			p.createCanvas(400,400);
		};
		p.draw = () => {
			p.background(220);
			p.fill('orange');
			p.stroke(0,200,0);
			p.strokeWeight(4);
			p.ellipse(200, 200, 200)
		};
	}
	new p5(mySketch,document.getElementById('sketch'));
</script>

